<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox style="-fx-alignment: center;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ThriftyRent.controller.AddVehicle">
    <padding>
        <Insets bottom="20.0" left="10.0" right="20.0" top="10.0" />
    </padding>
    <Label layoutX="81.0" layoutY="383.0" prefHeight="17.0" prefWidth="426.0" text="Vehicle Type">
        <VBox.margin>
        <Insets bottom="5.0" top="5.0" />
        </VBox.margin>
    </Label>
    <HBox>
        <ComboBox fx:id="vehicleType" layoutX="84.0" layoutY="14.0" prefHeight="25.0" prefWidth="251.0" VBox.vgrow="ALWAYS" />
    </HBox>
    <Label layoutX="81.0" layoutY="42.0" prefHeight="17.0" prefWidth="427.0" style="-fx-min-width: 100%;" text="Vehicle Id" AnchorPane.leftAnchor="81.0">
        <VBox.margin>
        <Insets bottom="5.0" top="5.0" />
        </VBox.margin>
    </Label>
    <TextField fx:id="id" layoutX="80.0" layoutY="59.0" prefHeight="25.0" prefWidth="212.0" AnchorPane.leftAnchor="80.0" AnchorPane.rightAnchor="80.0">
        <VBox.margin>
        <Insets />
        </VBox.margin>
    </TextField>
    <Label layoutX="81.0" layoutY="173.0" prefHeight="17.0" prefWidth="430.0" text="Make" AnchorPane.leftAnchor="81.0">
        <VBox.margin>
        <Insets bottom="5.0" top="5.0" />
        </VBox.margin>
    </Label>
    <TextField fx:id="make" layoutX="80.0" layoutY="190.0" prefHeight="25.0" prefWidth="212.0" AnchorPane.leftAnchor="80.0" AnchorPane.rightAnchor="80.0">
        <VBox.margin>
        <Insets />
        </VBox.margin>
    </TextField>
    <Label layoutX="81.0" layoutY="341.0" prefHeight="17.0" prefWidth="429.0" text="Model" AnchorPane.leftAnchor="81.0">
        <VBox.margin>
        <Insets bottom="5.0" top="5.0" />
        </VBox.margin>
    </Label>
    <TextField fx:id="model" layoutX="80.0" layoutY="358.0" prefHeight="25.0" prefWidth="212.0" AnchorPane.leftAnchor="80.0" AnchorPane.rightAnchor="80.0">
        <VBox.margin>
        <Insets />
        </VBox.margin>
    </TextField>
    <Label layoutX="81.0" layoutY="383.0" prefHeight="17.0" prefWidth="426.0" text="Year">
        <VBox.margin>
            <Insets bottom="5.0" top="5.0" />
        </VBox.margin>
    </Label>
    <TextField fx:id="year" layoutX="80.0" layoutY="400.0" prefHeight="25.0" prefWidth="440.0">
        <VBox.margin>
            <Insets />
        </VBox.margin>
    </TextField>

    <Label layoutX="81.0" layoutY="383.0" prefHeight="17.0" prefWidth="426.0" text="Select Image">
        <VBox.margin>
            <Insets bottom="5.0" top="5.0" />
        </VBox.margin>
    </Label>
    <HBox>

        <TextField fx:id="image" editable="false" layoutX="80.0" layoutY="400.0" prefHeight="25.0" prefWidth="414.0" />
        <Button onAction="#selectImage" text="...">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin></Button>
    </HBox>
    <Label layoutX="81.0" layoutY="383.0" prefHeight="17.0" prefWidth="426.0" text="Number Of Passengers">
        <padding>
        <Insets top="10.0" />
        </padding>
        <VBox.margin>
        <Insets bottom="5.0" top="5.0" />
        </VBox.margin>
    </Label>
    <HBox>
        <ComboBox fx:id="number_passengers" layoutX="84.0" layoutY="14.0" prefHeight="25.0" prefWidth="251.0" VBox.vgrow="ALWAYS" />
    </HBox>
    <Button layoutX="238.0" layoutY="443.0" mnemonicParsing="false" onAction="#addVehicle" prefHeight="25.0" prefWidth="124.0" style="-fx-alignment: center;" text="Add Vehicle" textAlignment="JUSTIFY" textOverrun="CLIP" AnchorPane.rightAnchor="238.0">
        <VBox.margin>
        <Insets top="10.0" />
        </VBox.margin>
    </Button>
</VBox>
